public boolean in3050(int a, int b) {
    if(a>50||b>50||a<30||b<30){
               return false;
           }
           if(a<=40&&b<=40){
               return true;
           }
           if(a<=50&&a>=40&&b<=50&&b>=40){
               return true;
           }
           
     return false;     
  }
